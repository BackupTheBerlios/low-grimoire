          MODULE=gnome-db
           MAJOR=0.2
         VERSION=0.2.96
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$MODULE/$MAJOR/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/$MODULE/$MAJOR/$SOURCE
        WEB_SITE=http://www.gnome-db.org
         ENTERED=20010922
         UPDATED=20020302
           SHORT="gnome-db provides a unified engine for accessing different databases."
cat << EOF
GNOME-DB is a unified engine for accessing database systems from
different database vendors by using a uniform and powerful API based
entirely on CORBA.
EOF
