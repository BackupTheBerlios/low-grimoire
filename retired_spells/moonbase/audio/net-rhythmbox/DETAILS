           MODULE=net-rhythmbox
         VERSION=0.4.6
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
SOURCE_URL[0]=http://web.verbum.org/net-rhythmbox/$SOURCE
        WEB_SITE=http://web.verbum.org/net-rhythmbox
         ENTERED=20030210
