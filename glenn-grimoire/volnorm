  VERSION=( "0.8.2" )
 CATEGORY="xmms-plugin audio"
ATTRIBUTE="library"
   SOURCE="$SPELL/$SPELL-$VERSION"
 HOMEPAGE="http://volnorm.sourceforge.net"
      URL="$SOURCEFORGE_URL"
      REQ=( "xmms" )
#FRESHMEAT=""
DESC="This is a volume normalizing plugin for XMMS.
This is a plugin for the MP3 player XMMS, it does volume normalizing so
that you will not need to play with the volume knob whenever a song
changes. It will set all songs to a fixed level that you set.
"
