   VERSION=( "1.03" "1.02" )
  CATEGORY="development"
 ATTRIBUTE="perl library"
    SOURCE="gtk2-perl/$SPELL-$VERSION"
  HOMEPAGE="http://www.goof.com/pcg/marc/"
       URL="$SOURCEFORGE_URL"
       REQ=( "perl glib" )
DESC="Package config for Gtk perl modules.
Package config for Gtk perl modules.
"

build() {
  perl  Makefile.PL  &&
  make               &&
  prepare_install    &&
  make    install
}
