  VERSION=( "0.6.5" "0.6.4" )
 CATEGORY="library"
ATTRIBUTE="library"
   SOURCE="$SPELL/$SPELL-$VERSION"
 HOMEPAGE="http://matroska.org"
      URL="http://dl.matroska.org/downloads"
      REQ=""
#FRESHMEAT=""
    SHORT="Library for reading ebml files (binary ML)."
LONG="
Library for reading ebml files (binary ML).
"

build() {
(
  cd make/linux               &&
  make                        &&
  prepare_install             &&
  make    install  prefix=/usr

) > $C_FIFO 2>&1}

