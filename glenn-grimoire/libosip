  VERSION=( "0.9.7" "0.9.6" )
 CATEGORY="library"
ATTRIBUTE="library"
   SOURCE="osip/$SPELL-$VERSION"
 HOMEPAGE="http://www.fsf.org/software/osip/osip.html"
      URL="$GNU_URL"
      REQ=""
#FRESHMEAT=""
DESC="The GNU oSIP library is an implementation of SIP - rfc3261.
The GNU oSIP library is an implementation of SIP - rfc3261.
"

build() {

  
  ./configure --prefix=/usr              \
	      --enable-gpref             \
	      --enable-pthread           \
              $OPTS                      &&
  make                                   &&
  prepare_install                        &&
  make install

}
