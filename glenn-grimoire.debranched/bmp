      URL="$SOURCEFORGE_URL"
      REQ=( "libglade id3lib"
            "GConf:--enable-gconf::enable GConf support"
            "gnome-vfs:--enable-gnome-vfs::enable GnomeVFS support"
            "::--disable-oss:enable oss output plugin"
            "alsa-lib::--disable-alsa:enable oss output plugin"
            "esound::--disable-esd:enable esound output plugin"
            "libogg libvorbis::--disable-vorbis:enable ogg vorbis input plugin" )
