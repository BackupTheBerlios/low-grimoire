  VERSION=( "1.2.6" )
 CATEGORY="development"
ATTRIBUTE="library"
 HOMEPAGE="http://www.gdal.org"
   SOURCE="$SPELL-$VERSION"
      URL="http://www.gdal.org/dl
           http://dl.maptools.org/dl/gdal
	   ftp://ftp.remotesensing.org/gdal"
      REQ=( "zlib"
            "tiff:::add tiff support"
            "jpeg:::add jpeg support"
            "libpng:::add png support"
            "postgresql:::add postgres support"
            "mysql:::add mysql support"
            "unixODBC:::add odbc support"
            "sqlite:::add SQLite support"
            "netcdf:--with-netcdf::add netcdf support"
            "cfitsio:--with-cfitsio::add FITS support"
            "libcsf:--with-pcraster::add PCRaster support"
            "ogdi:--with-ogdi::add OGDI support"
            "geos:--with-geos::add GEOS support"
            "Python:::add python support" )
 ESTIMATE="115"
FRESHMEAT="gdal"
DESC="GDAL is a translator library for raster geospatial data formats.
