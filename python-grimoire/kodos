   VERSION=( "2.4.5" )
  CATEGORY="development"
 ATTRIBUTE="kde python"
    SOURCE="$SPELL/$SPELL-$VERSION"
  HOMEPAGE="http://kodos.sourceforge.net"
       URL="$SOURCEFORGE_URL"
       REQ="PyQt"
