  VERSION=( "0.1.5" )
 CATEGORY="emulator"
ATTRIBUTE="x11"
   SOURCE="$SPELL/$SPELL-$VERSION"
 HOMEPAGE="http://fakenes.sourceforge.net/"
      URL="$SOURCEFORGE_URL"
      REQ="allegro zlib"
