   VERSION=( "1.00" )
  CATEGORY="development"
 ATTRIBUTE="perl library"
    SOURCE="gtk2-perl/$SPELL-Perl-$VERSION"
  HOMEPAGE="http://gtk2-perl.sourceforge.net"
       URL="$SOURCEFORGE_URL"
       REQ="perl Gtk2 gtkspell"
     SHORT="Perl bindings to the 2.x series of the GtkSpell graphical user interface library."
