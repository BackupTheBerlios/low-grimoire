unset OPT  
  VERSION=( "0.18" )
 CATEGORY="gkrellm-plugin"
ATTRIBUTE="concurrent x11"
   SOURCE="gkrellm_snmp-$VERSION"
 HOMEPAGE="http://triq.net/gkrellm_snmp.html"
      URL="http://triq.net/gkrellm"
